{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile",
    "dev": "npm run ts ./src/main.rest.ts",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "cli": "ts-node ./src/cli/main.ts",
    "cli.js": "npm run compile && node ./dist/cli/main.js",
    "mock.server": "json-server ./mocks/mock-server-data.json --port 3333"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "dayjs": "1.11.10",
    "inversify": "6.0.1",
    "pino": "8.16.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/node": "18.17.17",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "got": "13.0.0",
    "json-server": "0.17.4",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  }
}
